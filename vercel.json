{
  "builds": [
    { "src": "api/index.php", "use": "vercel-php@0.7.0" },
    { "src": "admin.php", "use": "vercel-php@0.7.0" },
    { "src": "blog.php", "use": "vercel-php@0.7.0" },
    { "src": "index.php", "use": "vercel-php@0.7.0" },
    { "src": "webstories.php", "use": "vercel-php@0.7.0" }
  ],
  "routes": [
    { "src": "/admin", "dest": "/admin.php" },
    { "src": "/blog", "dest": "/blog.php" },
    { "src": "/webstories", "dest": "/webstories.php" },
    { "src": "/web-stories/(.*)", "dest": "/webstories.php" },
    { "src": "/api/(.*)", "dest": "/api/index.php" },
    { "src": "/(.*)", "dest": "/index.php" }
  ]
}
