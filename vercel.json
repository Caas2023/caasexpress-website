{
  "functions": {
    "api/index.php": { "runtime": "vercel-php@0.7.0" },
    "admin.php": { "runtime": "vercel-php@0.7.0" },
    "blog.php": { "runtime": "vercel-php@0.7.0" },
    "index.php": { "runtime": "vercel-php@0.7.0" },
    "webstories.php": { "runtime": "vercel-php@0.7.0" }
  },
  "routes": [
    { "src": "/admin", "dest": "/admin.php" },
    { "src": "/blog", "dest": "/blog.php" },
    { "src": "/webstories", "dest": "/webstories.php" },
    { "src": "/web-stories/(.*)", "dest": "/webstories.php" },
    { "src": "/api/(.*)", "dest": "/api/index.php" },
    { "src": "/(.*)", "dest": "/index.php" }
  ]
}
